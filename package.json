    "lint": "tslint -p ./ -t verbose",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "comment-json": "^1.1.3",
    "dedent": "^0.7.0",
    "extended-yo-generator": "^1.0.5",
    "fs-extra": "^7.0.1",
    "lodash.kebabcase": "^4.1.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@types/comment-json": "^1.1.1",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^5.0.4",
    "@types/lodash.kebabcase": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/yeoman-test": "^2.0.0",
    "@types/yosay": "0.0.29",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
