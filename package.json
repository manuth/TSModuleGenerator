    "lint": "tslint -p ./ -t verbose",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "comment-json": "^1.1.3",
    "dedent": "^0.7.0",
    "extended-yo-generator": "^1.0.8",
    "fs-extra": "^8.0.1",
    "lodash.kebabcase": "^4.1.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@types/comment-json": "^1.1.1",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^7.0.0",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/npm-which": "^3.0.0",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "0.0.29",
    "mocha": "^6.1.4",
    "npm-which": "^3.0.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
